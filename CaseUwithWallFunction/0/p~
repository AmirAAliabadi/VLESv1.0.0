FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
	type            calculated;
        value           $internalField;    
    }

    outlet
    {
	type            calculated;
        value           $internalField;    
    }

    bottom
    {
        type            calculated;
        value           $internalField; 
    }

    top
    {
        type 		zeroGradient;
    }

    front
    {
        type cyclic;
    }

    back
    {
        type cyclic;
    }
}

// ************************************************************************* //
